    "consolidation/robo": "~1"
  },
  "autoload": {
    "classmap": [
      "scripts/composer/",
      "scripts/Robo/"
    ],
    "psr-4":{
      "Robo\\":"src"
    }
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "scripts": {
    "cs": "Utilities\\composer\\CodeStyle::cs",
    "cbf": "Utilities\\composer\\CodeStyle::cbf",
    "build": [
      "@composer install --no-progress --no-interaction",
      "@_start-vm",
      "@build-db"
    ],
    "rebuild": [
      "@cleanup",
      "@build"
    ],
    "rebuild-full": [
      "@cleanup-full",
      "@build"
    ],
    "build-db": [
      "drush @local.wellnesspolygon.com.ua sql-drop -y",
      "gunzip -c .data/wellnewsspolygon.sql.gz | drush @local.wellnesspolygon.com.ua sql-cli",
      "drush @local.wellnesspolygon.com.ua en wp_core -y",
      "drush @local.wellnesspolygon.com.ua updb -y"
    ],
    "cleanup": [
      "Utilities\\composer\\DrupalSettings::delete",
      "@_remove-drupal-docroot",
      "@_remove-dependencies"
    ],
    "cleanup-full": [
      "@cleanup",
      "@_remove-vm"
    ],
    "build-theme": "@_local-exec-grunt",
    "_build-theme-prod": "@composer build-theme -- prod",
    "_remove-core-gitignore": "@composer _unlink-recursive -- docroot/.gitignore",
    "_remove-dependencies": [
      "@composer _unlink-recursive -- vendor",
      "@composer _unlink-recursive -- node_modules"
    ],
    "_remove-drupal-docroot": "Utilities\\composer\\General::unlinkDocroot",
    "_start-vm": "@composer _global-exec -- ignore-error vagrant up",
    "_remove-vm": [
      "@composer _global-exec -- ignore-error vagrant destroy -f",
      "@composer _unlink-recursive -- .vagrant",
      "@composer _unlink-recursive -- .beetbox",
      "@composer _unlink-recursive -- .Vagrantfile"
    ],
    "_unlink-recursive": "Utilities\\composer\\General::unlinkRecursiveScript",
    "_global-exec": "Utilities\\composer\\General::globalExec",
    "_local-exec-grunt": "Utilities\\composer\\General::localExecGrunt",
    "post-install-cmd": [
      "@_remove-core-gitignore",
      "Utilities\\composer\\DrupalSettings::create",
      "npm install",
      "npm rebuild node-sass",
      "@_build-theme-prod"
    ],
    "post-update-cmd": [
      "@_remove-core-gitignore",
      "Utilities\\composer\\DrupalSettings::create",
      "npm install",
      "npm rebuild node-sass",
      "@_build-theme-prod"
    ],
    "deploy-artefact": "robo --ansi --load-from $(pwd)/scripts/Robo/BuildArtefact.php artefact"
  },
  "extra": {
    "installer-paths": {
      "docroot": ["type:drupal-core"],
      "docroot/sites/all/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/sites/all/themes/contrib/{$name}": ["type:drupal-theme"],
      "docroot/sites/all/libraries/{$name}": ["type:drupal-library"],
      "docroot/sites/all/drush/{$name}": ["type:drupal-drush"]
    },
    "preserve-paths": [
      "docroot/sites/all/modules/custom",
      "docroot/sites/all/themes/custom",
      "docroot/sites/all/drush",
      "docroot/sites/default/settings.php",
      "docroot/sites/default/files"
    ],
    "patches": {
      "drupal/features": [
        {
          "title": "Notice: Undefined property: stdClass::$status in features_export_form",
          "url": "https://www.drupal.org/files/issues/undefined-property-status-2324973-19.patch"
        }
      ],
      "drupal/features_banish": [
        {
          "title": "Add support for wildcards",
          "url": "https://www.drupal.org/files/issues/add-wildcard-support-2494417-4.patch"
        }
      ],
      "drupal/persistent_update": [
        {
          "title": "Add an ability to bypass all persistent updates",
          "url": "https://www.drupal.org/files/issues/bypass-all-persistent-updates-2824493-3.patch"
        }
      ]
    },
    "mouf": {
      "nodejs": {
        "forceLocal": true
      }
    }
  }
}
